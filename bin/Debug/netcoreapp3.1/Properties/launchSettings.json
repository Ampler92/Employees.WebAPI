{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:58436",
      "sslPort": 44336
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "EmployeeManagerAPI",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "EmployeeManager.WebAPI": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "EmployeeManagerAPI",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:44336/EmployeeManagerAPI",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:44336;https://+:5000",
        "ASPNETCORE_HTTPS_PORT": "44336"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}